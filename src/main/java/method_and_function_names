save - saveRejectedMessage
// функция, которая сохраняет информацию об отклонённых сообщениях
// для улучшения имени функции дополнил его объектом, над которым выполняется операция

saveMessage - saveAndProduceMessage
// функция, которая сохраняет сообщение в бд и отправляет сообщение в кафку
// для улучшения имени функции описал всё, что функция выполняет

checkExists - checkExistsMessage
// функция, которая проверяет существует ли уже такое сообщение в бд
// для улучшения имени функции дополнил его объектом, над которым выполняется операция

delete - deleteUserById
// новое имя уточняет, что метод удаляет пользователя по его идентификатору

log - logErrorMessage
// новое имя указывает, что метод регистрирует сообщение об ошибке

openFile- openFileForReading
// новое имя уточняет, что метод открывает файл для чтения

bind - bindFileToMessage
// новое имя уточняет, что метод привязывает файл к сообщению

deleteArchive - deleteArchiveFilesAndFilesContent
// новое имя указывает, что метод удаляет запись о файле, а также сожержимое этого файла

incrementCounter - incrementReadCount
// новое имя уточняет, что метод увеличаевает счётчик чтения файла

getNumber - generateRandomNumber
// новое имя описывает уточняет, что метод генерирует рандомное число, вместо неоднозначного имени

calc - calculateTotalCost
// новое имя описывает указывает, что метод рассчитывает итоговую стоимость, вместо неоднозначного имени

modifyStatus - setStatus
// новое имя четко указывает на установку статуса, вместо неочевидного modify